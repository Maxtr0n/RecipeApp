{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "SqlServer": "Server={DB_HOST};Database={DB_NAME};User Id={DB_USER};Password={DB_PASSWORD};Persist Security Info=False;TrustServerCertificate=True;",
    "Postgres": "Host=my_host;Port=5432;Database=my_database;Username=my_user;Password=my_password;"
  },
  "Keycloak": {
    "AuthorizationUrl": "http://localhost:18080/realms/recipe-app/protocol/openid-connect/auth"
  },
  "Authentication": {
    "MetadataAddress": "http://localhost:18080/realms/recipe-app/.well-known/openid-configuration",
    "ValidIssuer": "http://localhost:18080/realms/recipe-app",
    "Audience": ""
  }
}
